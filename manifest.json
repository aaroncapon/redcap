{
  "name": "run-grouplevel",
  "label": "FSL FEAT Run Grouplevel",
  "description": "a gear to modify tags",
  "maintainer": "Michael Perry <lmperry@stanford.edu>",
  "author": "Loki, the trickster god",
  "url": "http://fsl.fmrib.ox.ac.uk/fsl/fslwiki/BET",
  "source": "https://github.com/scitran-apps/fsl-bet",
  "license": "MIT",
  "version": "0.0.0_alpha17",
  "custom": {
    "flywheel": {
      "suite": "FSL"
    },
    "docker-image": "flywheel/group_feat:0.0.0",
    "gear-builder": {
      "category": "analysis",
      "image": "flywheel/group_feat:0.0.0"
    }
  },
  "config": {
    "Gear Log Level": {
      "default": "debug",
      "description": "Gear Log verbosity level (ERROR|WARNING|INFO|DEBUG)",
      "type": "string",
                        "enum": [
                                "error",
                                "warning",
                                "info",
                                "debug"
                        ]
      
    },
    "Analysis Filter": {
      "description": "regular expression (or direct string match) used to identify which analysis to use on the specified containers",
      "type": "string",
      "optional": true
    },
    "File Filter": {
      "description": "regular expression (or direct string match) to identify which file of the analysis to download",
      "type": "string",
      "optional": true
    }
 
  },
  "inputs": {
    "api_key": {
          "base": "api-key",
          "read-only": true
      },
    "fsf_file": {
      "base": "file",
      "description": "the design.fsf file created using input from the 'FSL FEAT Create Groups' Gear",
      "optional": false,
      "type": {
        "enum": [
          "text",""
        ]
      }
    }
},
  "environment": {
    "PYTHONPATH": "/flywheel/v0",
    "USER": "flywheel"
    
  },
  "command": "python3.6 /flywheel/v0/run.py"
}

